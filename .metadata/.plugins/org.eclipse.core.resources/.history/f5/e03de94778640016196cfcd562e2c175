package com.kalkan.calculator.calcopsum;

import com.kalkan.calculator.calculatorcore.Sum;

public class SumImpl implements Sum{
	private int a;
	private int b;
	private int result;
	
	public SumImpl(){
	}	
	public SumImpl(int a, int b){
		this.a = a;
		this.b = b;
		this.result = -1;
	}
	
	public int sum2(int a, int b) {		
		this.result = a + b;
		return a + b;
	}

	public void setA(int a){
		this.a = a;
	}	
	public void setB(int b){
		this.b = b;
	}
}
